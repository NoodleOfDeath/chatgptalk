{
  "name": "theskoop-mobile",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "postinstall": "sed -i '' 's/IPHONEOS_DEPLOYMENT_TARGET = 11.0/IPHONEOS_DEPLOYMENT_TARGET = 12.4/' ios/Pods/Pods.xcodeproj/project.pbxproj",
    "android": "react-native run-android",
    "ios": "react-native run-ios",
    "start": "react-native start",
    "test": "jest",
    "gen": "npx swagger-typescript-api -p ./swagger.json -o ./src/gen --no-client",
    "swagger": "(cd ../server && yarn swagger) && cp ../server/public/swagger.json . && yarn gen",
    "lint": "eslint .  --ext .js,.jsx,.ts,.tsx",
    "format": "eslint .  --ext .js,.jsx,.ts,.tsx --fix"
  },
  "dependencies": {
    "@react-navigation/bottom-tabs": "^6.5.7",
    "@react-navigation/native": "^6.1.6",
    "@react-navigation/native-stack": "^6.9.12",
    "axios": "^1.3.4",
    "date-fns": "^2.29.3",
    "debounce": "^1.2.1",
    "expo": "^48.0.6",
    "expo-constants": "^14.2.1",
    "expo-modules-core": "^1.2.4",
    "react": "18.2.0",
    "react-native": "0.71.3",
    "react-native-elements": "^3.4.3",
    "react-native-gesture-handler": "^2.9.0",
    "react-native-linear-gradient": "^2.6.2",
    "react-native-popup-menu": "^0.16.1",
    "react-native-reanimated": "^3.0.2",
    "react-native-reanimated-carousel": "^3.3.0",
    "react-native-safe-area-context": "^4.5.0",
    "react-native-screens": "^3.20.0",
    "react-native-vector-icons": "^9.2.0"
  },
  "devDependencies": {
    "@babel/core": "^7.20.0",
    "@babel/preset-env": "^7.20.0",
    "@babel/runtime": "^7.20.0",
    "@react-native-community/eslint-config": "^3.2.0",
    "@tsconfig/react-native": "^2.0.3",
    "@types/debounce": "^1.2.1",
    "@types/jest": "^29.2.1",
    "@types/node": "^18.14.6",
    "@types/react": "^18.0.24",
    "@types/react-native-vector-icons": "^6.4.13",
    "@types/react-test-renderer": "^18.0.0",
    "babel-jest": "^29.2.1",
    "jest": "^29.2.1",
    "metro-react-native-babel-preset": "0.73.7",
    "react-native-dotenv": "^3.4.8",
    "react-test-renderer": "18.2.0",
    "typescript": "4.8.4"
  },
  "jest": {
    "preset": "react-native"
  }
}
