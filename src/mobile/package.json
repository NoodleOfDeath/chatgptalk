{
  "name": "readless-mobile",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "postinstall": "sed -i '' 's/IPHONEOS_DEPLOYMENT_TARGET = 11.0/IPHONEOS_DEPLOYMENT_TARGET = 12.4/' ios/Pods/Pods.xcodeproj/project.pbxproj && yarn tsoa && yarn hack",
    "hack": "node_modules/.bin/rn-nodeify --hack --yarn",
    "android": "react-native run-android",
    "ios": "react-native run-ios",
    "start": "react-native start",
    "test": "jest",
    "gen": "npx swagger-typescript-api -p ./swagger.json -o ./src/gen && yarn cp:core",
    "cp:core": "if [[ ! -d ./src/core ]]; then mkdir -p ./src/core; fi && cp -rf ../core/client/* ./src/core",
    "tsoa": "(cd ../server && yarn tsoa) && cp ../server/swagger.json . && yarn gen",
    "lint": "eslint . --ext .js,.jsx,.ts,.tsx",
    "format": "eslint . --ext .js,.jsx,.ts,.tsx --fix"
  },
