{
  "name": "readless-mobile",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "postinstall": "yarn tsoa && yarn fix:pod",
    "fix:pod": "sed -i '' 's/IPHONEOS_DEPLOYMENT_TARGET = 11.0/IPHONEOS_DEPLOYMENT_TARGET = 12.4/' ios/Pods/Pods.xcodeproj/project.pbxproj",
    "android": "react-native run-android",
    "ios": "react-native run-ios",
    "start": "react-native start",
    "startr": "react-native start --resetCache",
    "build:ios": "react-native bundle --entry-file='./index.js' --bundle-output='./ios/ReadLess/main.jsbundle' --dev=false --platform='ios' --assets-dest='./ios'",
    "test": "jest",
    "gen": "npx swagger-typescript-api --disable-throw-on-error -p ./swagger.json -o ./src/gen && yarn cp:core",
    "cp:core": "if [[ -d src/core ]]; then rm -rf src/core; fi && mkdir -p ./src/core && cp -rf ../core/client/* ./src/core",
    "tsoa": "(cd ../server && yarn tsoa) && cp ../server/swagger.json . && yarn gen",
    "lint": "eslint . --ext .js,.jsx,.ts,.tsx",
    "format": "eslint . --ext .js,.jsx,.ts,.tsx --fix"
  },
  "dependencies": {
    "@react-native-async-storage/async-storage": "^1.18.1",
    "@react-native-google-signin/google-signin": "^9.0.2",
    "@react-navigation/bottom-tabs": "^6.5.7",
    "@react-navigation/native": "^6.1.6",
    "@react-navigation/native-stack": "^6.9.12",
    "@rneui/base": "^4.0.0-rc.7",
    "date-fns": "^2.29.3",
    "expo": "^48.0.0",
    "expo-constants": "^14.2.1",
    "expo-modules-core": "^1.2.4",
    "js-base64": "^3.7.5",
    "ms": "^2.1.3",
    "react": "18.2.0",
    "react-native": "0.71.3",
    "react-native-actions-sheet": "^0.8.29",
    "react-native-fs": "^2.20.0",
    "react-native-gesture-handler": "^2.9.0",
    "react-native-inappbrowser-reborn": "^3.7.0",
    "react-native-keyboard-aware-scroll-view": "^0.9.5",
    "react-native-motion": "^1.0.5",
    "react-native-reanimated": "^3.0.2",
    "react-native-reanimated-carousel": "^3.3.0",
    "react-native-safe-area-context": "^4.5.0",
    "react-native-screens": "^3.20.0",
    "react-native-share": "^8.2.1",
    "react-native-vector-icons": "^9.2.0",
    "react-native-view-shot": "^3.6.0"
  },
  "devDependencies": {
    "@babel/core": "^7.20.0",
    "@babel/preset-env": "^7.20.0",
    "@babel/runtime": "^7.20.0",
    "@react-native-community/eslint-config": "^3.2.0",
    "@tsconfig/react-native": "^2.0.3",
    "@types/jest": "^29.2.1",
    "@types/js-cookie": "^3.0.3",
    "@types/ms": "^0.7.31",
    "@types/node": "^18.14.6",
    "@types/react": "^18.0.24",
    "@types/react-native-vector-icons": "^6.4.13",
    "@types/react-test-renderer": "^18.0.0",
    "babel-jest": "^29.2.1",
    "jest": "^29.2.1",
    "metro-react-native-babel-preset": "0.73.7",
    "react-native-dotenv": "^3.4.8",
    "react-test-renderer": "18.2.0",
    "typescript": "4.8.4"
  },
  "jest": {
    "preset": "react-native"
  }
}
