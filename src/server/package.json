{
  "name": "readless-server",
  "version": "0.0.1",
  "export": "dist/index.cjs",
  "types": "dist/index.d.ts",
  "engines": {
    "node": ">=18.0.0"
  },
  "files": [
    "dist"
  ],
  "type": "module",
  "scripts": {
    "preinstall": "cd ../../ && yarn install && cd -",
    "start:api": "node dist/serve-api.cjs",
    "start:scheduler": "node dist/serve-scheduler.cjs",
    "start:worker": "node dist/serve-worker.cjs",
    "dev:api": "nodemon --config nodemon.api.json -x 'ts-node ./serve-api.ts'",
    "dev:scheduler": "nodemon --config nodemon.scheduler.json -x 'ts-node ./serve-scheduler.ts'",
    "dev:worker": "nodemon --config nodemon.worker.json -x 'ts-node ./serve-worker.ts'",
    "build:api": "rimraf dist/serve-api.cjs && esbuild ./src/api --bundle --platform=node --outfile=./dist/serve-api.cjs",
    "build:scheduler": "rimraf dist/serve-scheduler.cjs && esbuild ./src/scheduler --bundle --platform=node --outfile=./dist/serve-scheduler.cjs",
    "build:worker": "rimraf dist/serve-worker.cjs && esbuild ./src/worker --bundle --platform=node --outfile=./dist/serve-worker.cjs",
    "gen:site": "yarn test gen",
    "cp:core": "if [[ -d src/core ]]; then rm -rf src/core; fi && mkdir -p ./src/core/utils && cp -rf ../core/src/utils/* ./src/core/utils && cp -rf ../core/src/client/locales ./src/core/locales",
    "sql": "ts-node ./gen-sql-queries",
    "test": "jest",
    "tsoa": "tsoa spec -c tsoa.config.json",
    "lint": "eslint . --ext .js,.jsx,.ts,.tsx",
    "format": "eslint . --ext .js,.jsx,.ts,.tsx --fix"
  },
  "dependencies": {
    "@aws-sdk/client-s3": "^3.282.0",
    "@google-cloud/translate": "^7.2.1",
    "@tsed/di": "^7.17.0",
    "@types/sentiment": "^5.0.1",
    "aws-sdk": "^2.1334.0",
    "axios": "^1.3.4",
    "bcryptjs": "^2.4.3",
    "body-parser": "^1.20.2",
    "cheerio": "1.0.0-rc.12",
    "cors": "^2.8.5",
    "date-fns": "^2.30.0",
    "express": "^4.18.2",
    "express-validator": "^6.15.0",
    "express-winston": "^4.2.0",
    "firebase-admin": "^11.10.1",
    "google-auth-library": "^8.7.0",
    "googleapis": "^113.0.0",
    "jsonwebtoken": "^9.0.0",
    "mime-types": "^2.1.35",
    "ms": "^2.1.3",
    "nodemailer": "^6.9.1",
    "openai": "^3.3.0",
    "pg": "^8.9.0",
    "pg-hstore": "^2.3.4",
    "puppeteer": "^19.10.1",
    "sentiment": "^5.0.2",
    "sequelize": "^6.29.0",
    "sequelize-typescript": "^2.1.5",
    "sharp": "^0.32.6",
    "swagger-ui-express": "^4.6.2",
    "tsoa": "^5.1.1",
    "user-agents": "^1.0.1307",
    "uuid": "^9.0.0",
    "web3": "^1.10.0",
    "winston": "^3.8.2"
  },
  "devDependencies": {
    "@jest/globals": "^29.4.3",
    "@tsed/common": "^7.17.0",
    "@types/bcryptjs": "^2.4.2",
    "@types/body-parser": "^1.19.2",
    "@types/cors": "^2.8.13",
    "@types/date-fns": "^2.6.0",
    "@types/express": "^4.17.17",
    "@types/express-serve-static-core": "^4.17.33",
    "@types/jest": "^29.4.0",
    "@types/jsonwebtoken": "^9.0.1",
    "@types/mime-types": "^2.1.1",
    "@types/node": "^18.14.4",
    "@types/nodemailer": "^6.4.7",
    "@types/pg": "^8.6.6",
    "@types/qs": "^6.9.7",
    "@types/swagger-ui-express": "^4.1.3",
    "@types/user-agents": "^1.0.2",
    "@types/uuid": "^9.0.1",
    "@types/validator": "^13.7.12",
    "dotenv": "^16.0.3",
    "esbuild": "^0.17.11",
    "jest": "^29.4.3",
    "mock-aws-s3": "^4.0.2",
    "module-alias": "^2.2.3",
    "nodemon": "^2.0.21",
    "rimraf": "^3.0.2",
    "ts-jest": "^29.0.5",
    "ts-node": "^10.9.1",
    "tsconfig-paths": "^4.2.0",
    "tscpaths": "^0.0.9",
    "typescript": "^5.1.6"
  }
}
