{
  "name": "theskoop-api",
  "version": "0.0.1",
  "type": "module",
  "export": "dist/index.cjs",
  "types": "dist/index.d.ts",
  "files": [
    "dist"
  ],
  "engines": {
    "node": ">=18.0.0"
  },
  "bugs": {
    "url": "https://github.com/noodleofdeath/theskoop/issues",
    "email": "github@noodleofdeath.com"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/noodleofdeath/theskoop.git"
  },
  "homepage": "https://github.com/noodleofdeath/theskoop",
  "scripts": {
    "preinstall": "npx only-allow pnpm",
    "start": "node dist/index.cjs",
    "dev": "nodemon -x 'ts-node ./serve.ts'",
    "gen": "pgtyped -c ./src/schema/config.json",
    "build": "rimraf dist && esbuild ./src --bundle --platform=node --outfile=./dist/index.cjs",
    "format": "prettier --write 'src/**/*.ts'",
    "lint": "tslint -p tsconfig.json -c tslint.json",
    "typecheck": "tsc -p tsconfig.build.json --noEmit",
    "test": "jest",
    "generate-docs": "typedoc",
    "swagger": "tsoa spec -c tsoa.config.json"
  },
  "husky": {
    "hooks": {
      "pre-commit": "pretty-quick --staged"
    }
  },
  "dependencies": {
    "@tsed/di": "^7.14.2",
    "@types/swagger-ui-express": "^4.1.3",
    "body-parser": "^1.20.1",
    "chatgpt": "^4.7.2",
    "cors": "^2.8.5",
    "esbuild": "^0.17.10",
    "express": "^4.18.2",
    "express-validator": "^6.14.3",
    "express-winston": "^4.2.0",
    "node-fetch": "^3.3.0",
    "node-html-parser": "^6.1.4",
    "pg": "^8.9.0",
    "pg-hstore": "^2.3.4",
    "reflect-metadata": "^0.1.13",
    "sequelize": "^6.28.0",
    "sequelize-typescript": "^2.1.5",
    "swagger-ui-express": "^4.6.1",
    "tsoa": "^5.1.1",
    "winston": "^3.8.2"
  },
  "devDependencies": {
    "@tsed/common": "^7.14.2",
    "@types/body-parser": "^1.19.2",
    "@types/cors": "^2.8.13",
    "@types/express": "^4.17.17",
    "@types/express-serve-static-core": "^4.17.33",
    "@types/jest": "^29.4.0",
    "@types/node": "^18.13.0",
    "@types/node-fetch": "^2.6.2",
    "@types/pg": "^8.6.6",
    "@types/qs": "^6.9.7",
    "@types/validator": "^13.7.12",
    "concurrently": "^7.6.0",
    "dotenv": "^16.0.3",
    "jest": "^29.4.3",
    "nodemon": "^2.0.20",
    "only-allow": "^1.1.1",
    "rimraf": "^3.0.2",
    "ts-jest": "^29.0.5",
    "ts-node": "^10.9.1",
    "tsconfig-paths": "^4.1.2",
    "tscpaths": "^0.0.9",
    "typescript": "^4.9.5"
  }
}
